{
  "name": "guratan",
  "version": "0.8.0",
  "description": "Tiny send tool for Google Drive",
  "author": "hankei6km <hankei6km@gmail.com> (https://github.com/hankei6km)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hankei6km/guratan.git"
  },
  "bugs": {
    "url": "https://github.com/hankei6km/guratan/issues"
  },
  "keywords": [
    "google",
    "drive"
  ],
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "guratan": "dist/main.js"
  },
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "npm run clean && tsc && rimraf dist/test && mv dist/src/* dist/ && rimraf dist/src",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "clean": "rimraf \"dist/*\"",
    "upgrade-interactive": "npm-check --update",
    "csb:test": "npm test -- --runInBand --watchAll"
  },
  "dependencies": {
    "@googleapis/drive": "^18.0.0",
    "strip-bom-stream": "^5.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.26",
    "@types/yargs": "^17.0.9",
    "jest": "^30.0.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
